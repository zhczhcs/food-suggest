/* pages/test/test.wxss */
.test-container {
  padding: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.test-title {
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
}
.test-checkbox-group {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-bottom: 30rpx;
}
.test-checkbox-label {
  margin: 0 20rpx 15rpx 0;
  font-size: 30rpx;
}
.test-result {
  font-size: 32rpx;
  color: #555;
}
